{
	"compilerOptions": {
		"module": "commonjs",
		"target": "es6",
		"outDir": "./dist/",
		"mapRoot": "./dist",
		"sourceRoot": "./src",
		"rootDir": "./src",
		"sourceMap": true,
		"removeComments": true,
	},
	"include": [
		"src/**/*.ts",
		"src/**/*.json"
	],
	"exclude": [
		"node_modules",
		"src/client"
	]
}