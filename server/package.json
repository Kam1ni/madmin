{
	"name": "madmin-server",
	"version": "0.1.0",
	"description": "Server for madmin server management platform",
	"main": "dist/index.js",
	"scripts": {
		"start": "node --enable-source-maps dist/index.js",
		"build": "tsc",
		"dev": "tsc-watch --onSuccess \"node dev-run.js\"",
		"migrate": "node dist/migration.js",
		"nexe": "node nexe-compile.js",
		"lint": "eslint . --fix --ext ts"
	},
	"lint-staged": {
		"./src/*": [
			"eslint . --fix --ext ts"
		]
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"author": "",
	"license": "MIT",
	"dependencies": {
		"axios": "^0.19.0",
		"body-parser": "^1.18.3",
		"cors": "^2.8.4",
		"express": "^4.17.3",
		"jsonwebtoken": "^8.3.0",
		"md5": "^2.2.1",
		"nedb": "^1.8.0",
		"randomstring": "^1.1.5"
	},
	"devDependencies": {
		"@types/bcrypt": "^2.0.0",
		"@types/body-parser": "^1.17.0",
		"@types/cors": "^2.8.6",
		"@types/express": "^4.16.0",
		"@types/jsonwebtoken": "^7.2.7",
		"@types/md5": "^2.1.33",
		"@types/nedb": "^1.8.6",
		"@types/node": "^14.0.22",
		"@types/randomstring": "^1.1.6",
		"@typescript-eslint/eslint-plugin": "^2.18.0",
		"@typescript-eslint/parser": "^3.6.0",
		"eslint": "^6.8.0",
		"husky": "^4.2.5",
		"lint-staged": "^10.2.11",
		"nexe": "^3.3.7",
		"tsc-watch": "^2.2.1",
		"typescript": "^3.7.2"
	}
}
